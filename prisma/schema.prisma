generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Bot {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  ticker    String   @unique
  strategy  String
  balance   Float
  isActive  Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  cash      Float
  userId    String   @db.ObjectId
}
